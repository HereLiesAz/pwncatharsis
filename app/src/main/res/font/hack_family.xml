<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/hack_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/hack_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        android:font="@font/hack_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/hack_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />
    <font
        android:font="@font/hack_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/hack_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
    <font
        android:font="@font/hack_bolditalic"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/hack_bolditalic"
        app:fontStyle="normal"
        app:fontWeight="700" />
</font-family>